@page
@using Volo.Abp.AspNetCore.Mvc.UI.Layout
@using Microsoft.AspNetCore.Mvc.Localization
@using Volo.Abp.Validation.Localization
@model EasyAbp.Abp.TagHelperPlus.Pages.Books.Book2.IndexModel
@inject IPageLayout PageLayout
@inject IHtmlLocalizer<AbpValidationResource> L
@{
    PageLayout.Content.Title = L["Menu:Book2"].Value;
    PageLayout.Content.MenuItemName = "Book2";
}

@section scripts
{
    <abp-script src="/Pages/Books/Book2/index.js" />
}
@section styles
{
    <abp-style src="/Pages/Books/Book2/index.css"/>
}

<abp-dynamic-form abp-model="ViewModel" data-ajaxForm="true" asp-page="Index">
    <abp-card>
        <abp-card-body>
            <abp-form-content></abp-form-content>
        </abp-card-body>
        <abp-modal-footer>
            <abp-button id="search-button" button-type="Primary" text="Search"/>
        </abp-modal-footer>
    </abp-card>
</abp-dynamic-form>